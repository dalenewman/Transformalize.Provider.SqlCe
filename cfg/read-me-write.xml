<add name='Bogus' mode='init' flatten='true'>
  <connections>
    <add name='input' provider='bogus' seed='1' />
    <add name='output' provider='sqlce' file='c:\temp\junk.sdf' />
  </connections>
  <entities>
    <add name='Contact' size='1000'>
      <fields>
        <add name='Identity' type='int' primary-key='true' />
